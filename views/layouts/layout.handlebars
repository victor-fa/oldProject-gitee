<!DOCTYPE html>
<html lang="en">

<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="齐悟 - 控制台">
    <meta name="author" content="人马互动科技有限公司">

    <title>齐悟 - 控制台</title>

    <link rel="icon" type="image/x-icon" href="/img/favicon.ico">

    <!-- Bootstrap Core CSS -->
    <link href="/css/util/bootstrap.min.css" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="/css/util/sb-admin.css" rel="stylesheet"> {{#each css}}
    <link rel="stylesheet" href="{{this}}" /> {{/each}}

    <!-- Custom Fonts -->
    <link href="/font-awesome/css/font-awesome.min.css" rel="stylesheet" type="text/css">


    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body>

    <div id="wrapper">

        <!-- Navigation -->
        <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
                <a class="navbar-brand" href="/">齐悟</a>
            </div>
            <!-- Top Menu Items -->
            <ul class="nav navbar-right top-nav">

                {{#if user}}
                <li class="dropdown">
                    <a href="#" class="dropdown-toggle" data-toggle="dropdown"><i class="fa fa-user"></i> {{user.name}} <b
                        class="caret"></b></a>
                    <ul class="dropdown-menu">
                        <li>
                            <a href="/users/alert"><i class="fa fa-fw fa-bell"></i> 提醒设置</a>
                        </li>
                        <li>
                            <a href="/users/account"><i class="fa fa-fw fa-gear"></i> 账户信息</a>
                        </li>
                        <li class="divider"></li>
                        <li>
                            <a href="/users/logout"><i class="fa fa-fw fa-power-off"></i> 登出</a>
                        </li>
                    </ul>
                </li>
                {{/if}}

            </ul>


            <!-- Sidebar Menu Items - These collapse to the responsive navigation menu on small screens -->

            <div class="collapse navbar-collapse navbar-ex1-collapse">
                <ul class="nav navbar-nav side-nav">
                    {{#if user}} {{#ifCond user.group '>=' 4}} {{!-- Manager and Admin Only --}}
                    <li>
                        <a data-toggle="collapse" data-target="#qw-admin-tool"><i class="fa fa-fw fa-paper-plane"></i> 管理员 <i class="fa fa-fw fa-caret-down"></i></a>
                        <ul id="qw-admin-tool" class="collapse">
                            <li>
                                <a href="/admin/manage"><i class="fa fa-user-plus" aria-hidden="true"></i> 用户管理</a>
                            </li>
                            <li>
                                <a href="#"><i class="fa fa-fw fa-cubes" aria-hidden="true"></i> APP管理</a>
                            </li>
                        </ul>
                    </li>
                    {{/ifCond}}
                    <li>
                        <a href="/"><i class="fa fa-fw fa-dashboard" aria-hidden="true"></i> 控制台</a>
                    </li>
                    <li>
                        <a href="/chart"><i class="fa fa-fw fa-bar-chart-o" aria-hidden="true"></i> 趋势图</a>
                    </li>
                    <li>
                        <a href="/log/log"><i class="fa fa-fw fa-file-text" aria-hidden="true"></i> 对话日志</a>
                    </li>
                    <li>
                        <a href="/test/manual"><i class="fa fa-fw fa-pencil-square-o" aria-hidden="true"></i> 手动测试</a>
                    </li>
                    <li>
                        <a href="/test/config"><i class="fa fa-fw fa-font" aria-hidden="true"></i> 自动测试</a>
                    </li>
                    <li>
                        <a href="/users/alert"><i class="fa fa-fw fa-bell" aria-hidden="true"></i> 提醒设置</a>
                    </li>
                    <li>
                        <a href="/users/account"><i class="fa fa-fw fa-gear" aria-hidden="true"></i> 账户信息</a>
                    </li>
                    <li>
                        <a href="/users/logout"><i class="fa fa-fw fa-power-off" aria-hidden="true"></i> 登出</a>
                    </li>
                    {{else}}
                    <li>
                        <a href="/users/login"><i class="fa fa-sign-in" aria-hidden="true"></i> 登录</a>
                    </li>
                    <li>
                        <a href="/users/register"><i class="fa fa-user" aria-hidden="true"></i> 新用户</a>
                    </li>
                    {{/if}}
                </ul>
            </div>
            <!-- /.navbar-collapse -->

        </nav>

        <div id="page-wrapper">
            {{{body}}}
        </div>
        <!-- /#page-wrapper -->

    </div>
    <!-- /#wrapper -->

    <!-- jQuery -->
    <script src="/js/util/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="/js/util/bootstrap.min.js"></script>


    <!-- Custom JavaScript  -->
    {{#each js}}
    <script src="{{this}}"></script>
    {{/each}}


</body>

</html>