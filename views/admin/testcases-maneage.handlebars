<div class="container-fluid">
    <!-- Page Heading -->
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">
                上传测试文件
            </h1>
            <ol class="breadcrumb">
                <li>
                    <i class="fa fa-dashboard"></i> <a href="/">控制台</a>
                </li>
                <li class="active">
                    <i class="fa fa-fw fa-cubes" aria-hidden="true"></i> 上传测试文件
                </li>
            </ol>
        </div>
    </div>
    <!-- /.row -->

    <!-- /.row -->
    <div class="row">
        <form ref='uploadForm' action="/image/upload" method="POST" encType="multipart/form-data">
            <div class="form-group" style="margin-left: 18px;">
                <div class="row">
                    <label class="col-sm-2 control-label">测试文件</label>
                </div>
                <div class="row">
                    <label class="col-sm-1 control-label"></label>
                    <input class="form-control" type="file" name="picture" id="picture" style="width: 240px;display: inline;">
                    <input class="btn btn-primary" type="submit" value="Upload" style="margin-bottom: 10px;">
                </div>
                <label class="checkbox-inline"></label>
            </div>
        </form>
    </div>
</div>